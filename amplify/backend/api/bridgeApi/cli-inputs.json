{
  "version": 1,
  "paths": {
    "/shortcut": {
      "name": "/shortcut",
      "lambdaFunction": "shortcuthandler",
      "permissions": {
        "setting": "private",
        "auth": [
          "create",
          "read",
          "update",
          "delete"
        ]
      }
    },
    "/profile": {
      "name": "/profile",
      "lambdaFunction": "profilehandler",
      "permissions": {
        "setting": "private",
        "auth": [
          "create",
          "read",
          "update",
          "delete"
        ]
      }
    }
  }
}